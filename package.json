{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@actions/glob": "^0.1.1",
    "@octokit/rest": "^18.0.9",
    "@types/line-column": "^1.0.0",
    "github-actions-parser": "^0.8.6",
    "line-column": "^1.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.20",
    "@typescript-eslint/parser": "^4.12.0",
    "@vercel/ncc": "^0.26.1",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.2"
  }
}
